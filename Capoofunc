end

function itemschangehack()
XY = gg.prompt({"请输入要改的物品代码\n输入12239恢复"},{[1]=10500},{[1]="number"})[1]
gg.toast(so({"噴漆改物",12240,4,4},{{200,-432,4},{2,0x8,4}},{{XY,-640,4,1}})) gg.toast("噴漆修改成功")end

local originalValuesc = {}
-- 還原函數
function restoreWorldToolItemBlocks()
  if #originalValuesc == 0 then
    gg.toast("nil")
    return
  end

  gg.setValues(originalValuesc)
  originalValuesc = {}
end


function setworldtoolitemblocks()
if xsd3 then
restoreWorldToolItemBlocks()
end
 xsd3 = gg.prompt(
    {"请输入你要改的编辑器填充物代码,三个祭台140,141,142,三个圣遗物587,588,589,卡图方块1049,老版本炸药桶835,刷怪方块1080"},
    {[1] = 835},
    {[1] = "number"}
  )[1]
  -- 搜索並獲取所有匹配結果
  gg.searchNumber("104", gg.TYPE_DOUBLE, false, gg.SIGN_EQUAL, 0, -1, 0)
  gg.processResume()
  gg.refineNumber("104", gg.TYPE_DOUBLE, false, gg.SIGN_EQUAL, 0, -1, 0)
  gg.processResume()
  
  -- 獲取搜尋結果並儲存原始值
  originalValuesc = gg.getResults(10000)
  
  -- 修改所有結果
  gg.editAll(xsd3, gg.TYPE_DOUBLE)

  gg.clearResults()
  gg.processResume()
  gg.toast("填充物修改成功")
end



ikun2 = 0
function autodohead1()
	ai2 = -180
	repeat
		ai2 = ai2 + AA3s2
		if ai2 > 180 then
			ai2 = ai2 - 360
		end
		gg.sleep(5)  -- 縮短延遲時間以提升流暢度
		
	 addresses = {
			{offsets = {0x2646A0, 0x368, 0x88}, freeze = true},
			{offsets = {0x2646A0, 0x250, 0x50}, freeze = true},
			{offsets = {0x2646A0, 0x250, 0x60}, freeze = true},
		}
		
		for _, addr in ipairs(addresses) do
			local t = {publicbase, "Cb"}
			local tt = addr.offsets
			local ttt = S_Pointer(t, tt, true)
			gg.addListItems({{address = ttt, flags = 16, value = ai2, freeze = addr.freeze}})
		end
	until (ikun2 > 0)

	for _, addr in ipairs(addresses) do
		local t = {publicbase, "Cb"}
		local tt = addr.offsets
		local ttt = S_Pointer(t, tt, true)
		gg.addListItems({{address = ttt, flags = 16, value = ai2, freeze = false}})
	end
end

function autodohead2()
ikun2=1
gg.toast("轉頭旋轉已關閉")
return
end


ikun3=0
function autodoheads1()
ai3 = -180
repeat
ai3 = ai3 + AA4s2
gg.sleep(20) 
local t = {publicbase, "Cb"}
local tt = {0x2646A0,0x368,0x8C}
local ttt = S_Pointer(t, tt, true)
gg.addListItems({{address = ttt, flags = 16, value = ai3, freeze = true}})
local t = {publicbase, "Cb"}
local tt = {0x2646A0,0x250,0x54}
local ttt = S_Pointer(t, tt, true)
gg.addListItems({{address = ttt, flags = 16, value = ai3, freeze = true}})
until (ai3 > 180)
if (ikun3 >0) then
local t = {publicbase, "Cb"}
local tt = {0x2646A0,0x368,0x8C}
local ttt = S_Pointer(t, tt, true)
gg.addListItems({{address = ttt, flags = 16, value = ai3, freeze = false}})
local t = {publicbase, "Cb"}
local tt = {0x2646A0,0x250,0x54}
local ttt = S_Pointer(t, tt, true)
gg.addListItems({{address = ttt, flags = 16, value = ai3, freeze = false}})
return
else
return autodoheads1()
end end

