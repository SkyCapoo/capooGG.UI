end

function setstar()
AA3 = gg.prompt({"输入星星數量"}
,{[1]=50},{[1]="number"})[1]
local t = {publicbase, "Cb"}
local tt = {0x2646A0,0x1A0,0x1E0,0x160,0x1F0}
local ttt = S_Pointer(t, tt, true)
AA3 = AA3*38+945804460
gg.addListItems({{address = ttt, flags = 4, value = AA3, freeze = true}})
gg.toast("星星修改成功")
gg.clearResults()
end

