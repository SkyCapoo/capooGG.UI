local publicbase = "liblibGameApp.so:bss"
local player_data_adress = 0x42FB8
local xdz={[1]="行走[通用]",[3]="死亡[通用]",[9]="睡覺[通用]",[12]="游泳[通用]",[15]="睡覺[通用]",[21]="滑鏟[通用]",[29]="吃東西[通用]",[36]="坐下[通用]",[53]="摸空气[通用]",[55]="抱坐[通用]",[65]="打篮球[通用]",[68]="展开双臂[通用]",[97]="双手举起[通用]",[-1]="恢复动作"}

function a()
	gg.alert(SJ)
end
function b()
local processes = gg.getListItems()
local gameProcess = nil
local gameProcessName = "mini world" -- 確保這是《迷你世界》的進程名稱

-- 使用 gg.getTargetInfo() 來獲取當前選中的進程信息
local targetInfo = gg.getTargetInfo()
local selectedProcess = targetInfo.label or ""

-- 如果當前選中的進程不是《迷你世界》，則選擇它
if string.find(selectedProcess:lower(), gameProcessName) then
    gg.alert("當前已選擇的進程是迷你世界: " .. selectedProcess)
else
    -- 遍歷進程列表，尋找包含 "miniworld" 的進程名稱
    for _, process in ipairs(processes) do
        if process.name ~= nil and string.find(process.name:lower(), gameProcessName) then
            gameProcess = process
            break
        end
    end

    -- 如果找到《迷你世界》進程，則選擇它
    if gameProcess then
        gg.setSelectedProcess(gameProcess.process)
        gg.alert("已選擇迷你世界進程: " .. gameProcess.name)
    else
        gg.alert("未找到包含 '" .. gameProcessName .. "' 的進程")
    end
end

end




-- 儲存原始數據的表
local originalValues = {}
local hitboxsize=120

function hitbox()
hitboxsize = gg.prompt({"輸入hitbox大小 200正常"},{[1]=1500},{[1]="number"})[1]
  function bf(qmnb, qmxg)
    gg.setVisible(false)
    gg.clearResults()
    qmnbv = qmnb[3]["value"] or qmnb[3][1]
    qmnbt = qmnb[3]["type"] or qmnb[3][2]
    qmnbn = qmnb[2]["name"] or qmnb[2][1]
    gg.setRanges(qmnb[1]["memory"] or qmnb[1][1])
    gg.searchNumber(qmnbv, qmnbt)
    gg.refineNumber(qmnbv, qmnbt)
    sz = gg.getResultCount()
    if sz == 0 then
      gg.toast(qmnbn .. "开启失败，请尝试重启游戏")
    else
      sl = gg.getResults(999999)
      for i = 1, sz do
        pdsz = true
        for v = 4, #qmnb do
          if pdsz == true then
            pysz = {{}}
            pysz[1].address = sl[i].address + (qmnb[v]["offset"] or qmnb[v][2])
            pysz[1].flags = qmnb[v]["type"] or qmnb[v][3]
            szpy = gg.getValues(pysz)
            tzszpd = tostring(qmnb[v]["lv"] or qmnb[v][1]):gsub(",", "")
            pyszpd = tostring(szpy[1].value):gsub(",", "")
            if tzszpd == pyszpd then
              pdjg = true
              pdsz = true
            else
              pdjg = false
              pdsz = false
            end
          end
        end
        if pdjg == true then
          szpy = sl[i].address
          for x = 1, #qmxg do
            xgsz = qmxg[x]["value"] or qmxg[x][1]
            xgpy = szpy + (qmxg[x]["offset"] or qmxg[x][2])
            xglx = qmxg[x]["type"] or qmxg[x][3]
            xgdj = qmxg[x]["freeze"] or qmxg[x][4]
            xg = {{address = xgpy, flags = xglx, value = xgsz}}

            -- 儲存原始值
            table.insert(originalValues, {address = xgpy, flags = xglx, value = gg.getValues({{address = xgpy, flags = xglx}})[1].value})

            if xgdj == true then
              xg[1].freeze = xgdj
              gg.addListItems(xg)
            else
              gg.setValues(xg)
            end
          end
          xgjg = true
        end
      end
      if xgjg == true then
        gg.toast(qmnbn .. "开启成功,共注入 2 条冻结数据")
      else
        gg.toast(qmnbn .. "开启失败，请尝试重启游戏")
      end
    end
  end

  bf(
    {
      {4},
      {"碰撞箱"},
      {773094113340, gg.TYPE_QWORD},
      {220, 16, gg.TYPE_DWORD},
    },
    {
      {hitboxsize, 12, gg.TYPE_DWORD, false},
      {hitboxsize, 16, gg.TYPE_DWORD, false},
      {hitboxsize, 20, gg.TYPE_DWORD, false},
    }
  )
  gg.toast("HITBOX開啟成功")
  gg.clearResults()
end

-- 還原函數
function hitboxrestore()
  if #originalValues == 0 then
    gg.toast("nil")
    return
  end

  gg.setValues(originalValues)
  gg.toast("HITBOX關閉成功")
  originalValues = {}
end

function infjump1()
local t = {publicbase, "Cb"}
local tt = {0x2646A0, 0x1C8, 0x198, 0x84}
local ttt = S_Pointer(t, tt, true)
gg.addListItems({{address = ttt, flags = 4, value = 1, freeze = true}})
gg.toast("連跳已開啟")
end

