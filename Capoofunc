local ttt = S_Pointer(t, tt, true)
myskin = GetValue(ttt,4)
gg.toast(so({"刷華麗變身",220,4,4},{{60,-4},{120,8},{180,-8},{120,4},{"1~200",-0x764},{"0~6",-0x438}},{{myskin,-0x764}}))
gg.toast("華麗變身刷取成功")
end

