function bullets1()
gg.toast("请选择鎖定开关")
F = gg.alert("📌请选择是否鎖定 ","🔒鎖定","🔓普通")
 if  F~=1 and F~=2 then
  gg.toast("你未选择开关默认取消")
    gg.toast("你未选择｛开 关｝")
  else 
  end
if  F== 1 then
changebullet = gg.prompt({"输入子彈數量:"}
,{[1]=114514},{[1]="number"})[1]
gg.clearResults()
local t = {publicbase, "Cb"}
local tt = {0x4D00E0,0x188,0xA0,0xC0,0x160,0x10,0x14}
local ttt = S_Pointer(t, tt, true)
gg.setValues({{address = ttt, flags = 4, value = changebullet}})
gg.addListItems({{address = ttt, flags = 4, value = changebullet, freeze = true }})
gg.setValues({{address = ttt, flags = 4, value = changebullet}})
gg.toast("鎖定子彈數量修改成功")

elseif  F== 2 then
changebullet = gg.prompt({"输入子彈數量:"}
,{[1]=114514},{[1]="number"})[1]
gg.clearResults()
local t = {publicbase, "Cb"}
local tt = {0x4D00E0,0x188,0xA0,0xC0,0x160,0x10,0x14}
local ttt = S_Pointer(t, tt, true)
gg.setValues({{address = ttt, flags = 4, value = changebullet}})
gg.addListItems({{address = ttt, flags = 4, value = changebullet, freeze = false }})
gg.setValues({{address = ttt, flags = 4, value = changebullet}})
gg.toast("子彈數量修改成功")
end
end

