function changename()
gg.toast("请选择鎖定开关")
F = gg.alert("📌请选择是否鎖定改名 ","🔒改名","🔓還原")
 if  F~=1 and F~=2 then
  gg.toast("你未选择开关默认取消")
    gg.toast("你未选择｛开 关｝")
  else 
  end
if  F== 1 then
    local configPath = "/sdcard/Capoo名字配置文件.txt"
    local file = io.open(configPath, "rb")
    local mznb = ""
    if file then
        local content = file:read("*a")
        file:close()
        mznb = content:match("🍀(.-)🍀") or ""
    end

    local mnmcc = gg.prompt({"請輸入新名稱"}, {[1] = mznb}, {[1] = "text"})[1]
    if not mnmcc or mnmcc == "" then
        gg.toast("未輸入有效名稱，操作取消")
        return
    end

    file = io.open(configPath, "w")
    if file then
        file:write("🍀" .. mnmcc .. "🍀")
        file:close()
    else
        gg.toast("無法寫入配置文件")
        return
    end

    local t = {publicbase, "Cb"}
    local tt = {player_data_adress, 0x49}
    local ttt = S_Pointer(t, tt, true)

    if not ttt then
        gg.toast("內存地址無效")
        return
    end

    -- 修改並凍結新名稱
    modifyAndFreezeString(ttt, mnmcc, 64)
    gg.toast("名字已修改為: " .. mnmcc)
elseif  F== 2 then
    if next(modifiedValues) == nil then
        gg.toast("沒有需要還原的內存值")
        return
    end

    -- 構造還原數據表
    local restoreValues = {}
    for address, originalValue in pairs(modifiedValues) do
        table.insert(restoreValues, {
            address = address,
            flags = gg.TYPE_BYTE,
            value = originalValue
        })
    end
     -- 解凍內存值
    gg.removeListItems(restoreValues)
    -- 還原原始內存值
    gg.setValues(restoreValues)
    -- 清空記錄
    modifiedValues = {}
    gg.toast("名字已解凍並還原")
end
end
function fpsaimbot1()
    minijuid = gg.prompt({"输入自瞄目標迷你號"},{[1]="1000"},{[1]="number"})[1]
if tonumber(minijuid)<1000000000 then
minijuidss = minijuid + 1000000000
else
minijuidss = minijuid
end
shutaim = 0
gg.setRanges(4)
local dataType = 4
local tb1 = {{minijuidss, 0},{1120403456,716},{1120403456,712},}
local tb2 = {{ 20 }, }
local tb3 = {{ 24 }, }
local tb4 = {{ 28 }, }
SearchWrite(tb1, tb2, dataType)
local nmbx=gg.getResults(1)
addrx = nmbx[1].address
	 sl=gg.getValues(nmbx)
nnbx=sl[1].value

SearchWrite(tb1, tb3, dataType)
local nmby=gg.getResults(1)
addry = nmby[1].address
	 sl=gg.getValues(nmby)
nnby=sl[1].value


SearchWrite(tb1, tb4, dataType)
local nmbz=gg.getResults(1)
addrz = nmbz[1].address 
	 sl=gg.getValues(nmbz)
nnbz=sl[1].value
while true do
if (shutaim == "1") then
os.exit()
end
--準心XY
local t = {publicbase, "Cb"}
local tt = {0x121B98,0x38,0x8}
local ttta = S_Pointer(t, tt, true)
local tt = {0x121B98,0x38,0xc}
local tttb = S_Pointer(t, tt, true)
--自己的座標
local tt = {0x2646A0, 0x1C8, 0x198, 0xBC}
local ttt = S_Pointer(t, tt, true)
local tttx = GetValue(ttt, 4)
local tt = {0x2646A0, 0x1C8, 0x198, 0xC0}
local ttt = S_Pointer(t, tt, true)
local ttty = GetValue(ttt, 4)
local tt = {0x2646A0, 0x1C8, 0x198, 0xC4}
local ttt = S_Pointer(t, tt, true)
local tttz = GetValue(ttt, 4)
--自瞄目標的座標
local tttoox = GetValue(addrx, 4)
local tttooy = GetValue(addry, 4)
local tttooz = GetValue(addrz, 4)
local angleAddr = {ttta,tttb}
local aimPos = {tttx,ttty,tttz}
local playerPos = {tttoox,tttooy,tttooz}
local AimFn = function(aimPos, playerPos, angleAddr)
local disx, disy, disz = aimPos[1] - playerPos[1], aimPos[2] - playerPos[2], aimPos[3] - playerPos[3]
local distance=math.sqrt(math.pow(disx,2)+math.pow(disz,2))
local setAngleX=math.deg(math.atan2(disx,disz))-180
local setAngleY=math.deg(math.atan2(disy,distance))

--gg.addListItems({{address = angleAddr[1],flags = 16,value = setAngleX, freeze = true},{address = angleAddr[2],flags = 16,value = setAngleY, freeze = true}})
gg.setValues({{address = angleAddr[1],flags = 16,value = setAngleX},{address = angleAddr[2],flags = 16,value = setAngleY}})

end
AimFn(aimPos, playerPos, angleAddr)
end
end

function fpsaimbot2()
shutaim = 1
end

function Actionaimbot1()
    minijuide = gg.prompt({"输入自瞄目標迷你號"},{[1]="1000"},{[1]="number"})[1]
 if tonumber(minijuide)<1000000000 then
minijuidnn = minijuide + 1000000000
else
minijuidnn = minijuide
end
shutactionaimbot = 0
gg.setRanges(4)
local dataType = 4
local tb1 = {{minijuidnn, 0},{1120403456,716},{1120403456,712},}
local tb2 = {{ 20 }, }
local tb3 = {{ 24 }, }
local tb4 = {{ 28 }, }
SearchWrite(tb1, tb2, dataType)
local nmbx=gg.getResults(1)
addrx = nmbx[1].address
	 sl=gg.getValues(nmbx)
nnbx=sl[1].value
SearchWrite(tb1, tb3, dataType)
local nmby=gg.getResults(1)
addry = nmby[1].address
	 sl=gg.getValues(nmby)
nnby=sl[1].value
SearchWrite(tb1, tb4, dataType)
local nmbz=gg.getResults(1)
addrz = nmbz[1].address 
	 sl=gg.getValues(nmbz)
nnbz=sl[1].value
while true do
local t = {publicbase, "Cb"}
local tt = {0x2646A0,0x368,0x88}
local tttxa = S_Pointer(t, tt, true)
local tt = {0x2646A0,0x250,0x50}
local tttxb = S_Pointer(t, tt, true)
local tt = {0x2646A0,0x250,0x60}
local tttxc = S_Pointer(t, tt, true)
--xpos
local tt = {0x2646A0,0x368,0x8c}
local tttya = S_Pointer(t, tt, true)
local tt = {0x2646A0,0x250,0x54}
local tttyb = S_Pointer(t, tt, true)
--ypos

local tt = {0x2646A0, 0x1C8, 0x198, 0xBC}
local ttt = S_Pointer(t, tt, true)
local tttx = GetValue(ttt, 4)
local tt = {0x2646A0, 0x1C8, 0x198, 0xC0}
local ttt = S_Pointer(t, tt, true)
local ttty = GetValue(ttt, 4)
local tt = {0x2646A0, 0x1C8, 0x198, 0xC4}
local ttt = S_Pointer(t, tt, true)
local tttz = GetValue(ttt, 4)
--自瞄目標的座標
local tttoox = GetValue(addrx, 4)
local tttooy = GetValue(addry, 4)
local tttooz = GetValue(addrz, 4)

local angleAddra = {tttxa,tttya}
local angleAddrb = {tttxb,tttyb}
local aimPos = {tttx,ttty,tttz}
local playerPos = {tttoox,tttooy,tttooz}
if (shutactionaimbot == "1") then
gg.addListItems({{address = tttyb ,flags = 16,value = setAngleX, freeze = false}})
gg.addListItems({{address = angleAddra[1],flags = 16,value = setAngleX, freeze = false},{address = angleAddra[2],flags = 16,value = setAngleY, freeze = false}})
gg.addListItems({{address = angleAddrb[1],flags = 16,value = setAngleX, freeze = false},{address = angleAddrb[2],flags = 16,value = setAngleY, freeze = false}})
os.exit()
end
local AimFn = function(aimPos, playerPos, angleAddra)
local disx, disy, disz = aimPos[1] - playerPos[1], aimPos[2] - playerPos[2], aimPos[3] - playerPos[3]
local distance=math.sqrt(math.pow(disx,2)+math.pow(disz,2))
local setAngleX=math.deg(math.atan2(disx,disz))
local setAngleY=math.deg(math.atan2(disy,distance))

--gg.addListItems({{address = angleAddr[1],flags = 16,value = setAngleX, freeze = true},{address = angleAddr[2],flags = 16,value = setAngleY, freeze = true}})
gg.addListItems({{address = tttyb ,flags = 16,value = setAngleX, freeze = true}})
gg.addListItems({{address = angleAddra[1],flags = 16,value = setAngleX, freeze = true},{address = angleAddra[2],flags = 16,value = setAngleY, freeze = true}})
gg.addListItems({{address = angleAddrb[1],flags = 16,value = setAngleX, freeze = true},{address = angleAddrb[2],flags = 16,value = setAngleY, freeze = true}})

end
AimFn(aimPos, playerPos, angleAddra)
end
end

