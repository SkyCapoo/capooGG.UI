function changeteam()
teams = gg.prompt({"⬜0-白隊,🟥1-紅隊,🟦2-藍隊,🟩3綠隊,🟨4黃隊,🟧5橙隊,🟪6紫隊,👁️999旁觀者"}
,{[1]=1},{[1]="number"})[1]
local t = {publicbase, "Cb"}
local tt = {0x2646A0,0x1A0,0x300,0x70}
local ttt = S_Pointer(t, tt, true)
gg.setValues({{address = ttt, flags = 1, value = teams}})
end

local puttass = false
function player_TPCollision1()
stoptppd = 0
if puttass == false then
datauploadTime = gg.prompt({"输入傳送間隙時間"},{[1]="1"},{[1]="number"})[1]
gg.toast("傳送光環已開啟")
end
    while true do
    puttass = true
    local gameportal = gg.getRangesList(publicbase)[1].start
    local playerself_addr = GotoPointer(gameportal + 0x7B50, {0x88, 0xC0})
    local Table = {}
    for lid = 0, 39 do
         addr = playerself_addr + lid * 0x8
        addrX = GotoPointer(addr + 0x0 )
       testAs = GetValue(addrX + 0xB0,4)
         if testAs ~= 2 then
            return player_TPCollision1() end
         if (stoptppd == "1") then
         os.exit()
          return end
        Table[#Table + 1] = addr
        one_x = GetValue(addrX + 0x14,4)
        one_y = GetValue(addrX + 0x18,4)
        one_z = GetValue(addrX + 0x1c,4)
        miniUID = GetValue(addrX + 0x0,4)
--自己的座標
gg.sleep(datauploadTime)
gg.toast("迷你號:"..miniUID)
local t = {publicbase, "Cb"}
local tt = {0x2646A0, 0x1C8, 0x198, 0xBC}
local tttXX = S_Pointer(t, tt, true)

local tt = {0x2646A0, 0x1C8, 0x198, 0xC0}
local tttYY = S_Pointer(t, tt, true)

local tt = {0x2646A0, 0x1C8, 0x198, 0xC4}
local tttZZ = S_Pointer(t, tt, true)

local TPLocation = {tttXX,tttYY,tttZZ}
gg.setValues({{address = TPLocation[1],flags = 4,value = one_x},{address = TPLocation[2],flags = 4,value = one_y},{address = TPLocation[3],flags = 4,value = one_z}})
    end
    return Table, GotoPointer(playerself_addr)
end
end


function player_TPCollision2()
stoptppd = 1
puttass = false
gg.toast("傳送光環已關閉")
end

function ca1() 
a9aa=o5v
local t = {publicbase, "Cb"}
local tt = {0x4349C0,0x2B8,0x70,0x2D0,0x90}
local ttt = S_Pointer(t, tt, true)
gg.setValues({{address = ttt, flags = 4, value = a9a}})
gg.toast(a9aa.."人房間修改成功")
end

function ca2()
local Aa3=gg.prompt({"开启功能后,再开房间就行了"},{"40"},{"number"})
local num=tonumber(Aa3[1])
if Aa3 then if num>40 or num<1 then gg.alert("只能开启1~40人的房间") else
gg.toast(so({"大房间",1078198272,4,4},{{1077805056,-0x10},{1077149696,-0x20},{1076101120,-0x30},{1075052544,-0x50}},{{num,-0x44,64}}))
end end end

function changeroomplayerquantity()
SHff = gg.choice({
   "𒅒🔰開10人房間🔰𒅒",
   "𒅒🔰開20人房間🔰𒅒",
   "𒅒🔰開30人房間🔰𒅒",
   "𒅒🔰️開40人房間🔰𒅒",
   "𒅒🔰️自定義房間🔰𒅒",
},2018,os.date("𒅒使用前先選任意人數,開启成功后直接開房間就行了,建议每次開房間前都開一次这功能𒅒"))
if SHff==1 then a9a=7 o5v=10 ca1()end
if SHff==2 then a9a=8 o5b=20 ca1()end
if SHff==3 then a9a=9 o5v=30 ca1()end
if SHff==4 then a9a=10 o5v=40 ca1()end
if SHff==5 then ca2() end
XGCK=-1
end


local function getCoordinates(addr)
  local result = gg.getValues({{address = addr, flags = 4}})
  return result[1].value
end

-- 計算距離
local function calculateDistance(x1, y1, z1, x2, y2, z2)
  return math.sqrt((x2 - x1)^2 + (z2 - z1)^2)
end


-- 設定新的視角


function playerAimbot1()
readthevalueff = gg.prompt({"输入自瞄範圍,1=1格方塊","排除的隊伍(⬜0-白隊,🟥1-紅隊,🟦2-藍隊,🟩3綠隊,🟨4黃隊,🟧5橙隊,🟪6紫隊,👁️999旁觀者)"},{[1]="8",[2]="-1"},{[1]="number",[2]="number"})
theMaxdistance = readthevalueff[1] * 100
noaimtean = readthevalueff[2]
nearestDistance = theMaxdistance
nearestEnemy = nil
-- 準心XY
gg.toast("靠近玩家準心自瞄已開啟")
local t = {publicbase, "Cb"}
local tt = {0x121B98, 0x38, 0x8}
local ttta = S_Pointer(t, tt, true)
local tt = {0x121B98, 0x38, 0xc}
local tttb = S_Pointer(t, tt, true)

-- 自己的座標
local tt = {0x2646A0, 0x1C8, 0x198, 0xBC}
local tttx = S_Pointer(t, tt, true)
local tttx_value = GetValue(tttx, 4)
local tt = {0x2646A0, 0x1C8, 0x198, 0xC0}
local ttty = S_Pointer(t, tt, true)
local ttty_value = GetValue(ttty, 4)
local tt = {0x2646A0, 0x1C8, 0x198, 0xC4}
local tttz = S_Pointer(t, tt, true)
local tttz_value = GetValue(tttz, 4)

-- 設定敵人和玩家的座標內存地址（這些值需要你自己搜索和確認）
local enemyXAddr = one_x
local enemyYAddr = one_y
local enemyZAddr = one_z

local playerXAddr = tttx
local playerYAddr = ttty
local playerZAddr = tttz
-- 主循環
local portal = gg.getRangesList(publicbase)[1].start
local oneself_addr = GotoPointer(portal + 0x7B50, {0x88, 0xC0})
local function setViewAngle(disx, disy, disz, distance)
  local angleX = math.deg(math.atan2(disx, disz)) - 180
  local angleY = math.deg(math.atan2(disy, distance))
  gg.setValues({
    {address = ttta, flags = gg.TYPE_FLOAT, value = angleX},
    {address = tttb, flags = gg.TYPE_FLOAT, value = angleY}
  })
end
ikumstopaim = 0
while true do
local nearestDistance = theMaxdistance  -- 初始化最大距離
    local nearestEnemy = nil  -- 用來保存最近敵人的內存地址
    local nearestDisx, nearestDisy, nearestDisz = 0, 0, 0  -- 保存最近敵人的位置差異
  for lid = 0, 39 do
    local addr = oneself_addr + lid * 0x8
    local addrX = GotoPointer(addr + 0x0)
    local uidvalue = GetValue(addrX + 0x0, 4)
    local pdvalue = GetValue(addrX + 0xB0, 4)
    local pdteam = GetValue(addrX + 0x24, 4)
    local playerhp = GetValue(addrX + 0x10, 16)
    if ikumstopaim == 1 then
    os.exit()
    return end 
    if uidvalue ~= 0 and uidvalue ~= nil then
    if pdvalue > 0 and pdvalue < 3 then
    if playerhp >0 then
       one_x = addrX + 0x14
       one_y = addrX + 0x18
       one_z = addrX + 0x1C

      -- 讀取敵人和玩家的位置
      local enemyX = getCoordinates(one_x)
      local enemyY = getCoordinates(one_y)
      local enemyZ = getCoordinates(one_z)
      
      local playerX = getCoordinates(playerXAddr)
      local playerY = getCoordinates(playerYAddr)
      local playerZ = getCoordinates(playerZAddr)
      
      local disx = playerX - enemyX
      local disy = playerY - enemyY
      local disz = playerZ - enemyZ

      -- 計算距離
      local distance = calculateDistance(enemyX, enemyY, enemyZ, playerX, playerY, playerZ)
      
      -- 如果距離小於某個閾值，則自動瞄準
      if distance < nearestDistance then
      if pdteam ~= noaimtean then
            nearestDistance = distance
            nearestEnemy = addrX
            nearestDisx, nearestDisy, nearestDisz = disx, disy, disz
         
       end
       end
      end
    end
  end
 end
 if nearestEnemy~=nil then
 setViewAngle(nearestDisx, nearestDisy, nearestDisz, nearestDistance)
 end
  -- 等待一段時間再進行下一次檢測
  gg.sleep(1)
end
end




function playermvAimbot1()
readthevalueff = gg.prompt({"输入自瞄範圍,1=1格方塊","排除的隊伍(⬜0-白隊,🟥1-紅隊,🟦2-藍隊,🟩3綠隊,🟨4黃隊,🟧5橙隊,🟪6紫隊,👁️999旁觀者)"},{[1]="8",[2]="-1"},{[1]="number",[2]="number"})
theMaxdistance = readthevalueff[1] * 100
noaimtean = readthevalueff[2]
nearestDistance = theMaxdistance
nearestEnemy = nil
ikummvstopaim = 0
-- xpos
gg.toast("靠近玩家動作自瞄已開啟")
local t = {publicbase, "Cb"}
local tt = {0x2646A0,0x368,0x88}
local ttta = S_Pointer(t, tt, true)
local tt = {0x2646A0,0x250,0x50}
local tttb = S_Pointer(t, tt, true)
local tt = {0x2646A0,0x250,0x60}
local tttc = S_Pointer(t, tt, true)
--ypos
local tt = {0x2646A0,0x368,0x8c}
local tttaa = S_Pointer(t, tt, true)
local tt = {0x2646A0,0x250,0x54}
local tttbb = S_Pointer(t, tt, true)
-- 自己的座標
local tt = {0x2646A0, 0x1C8, 0x198, 0xBC}
local tttx = S_Pointer(t, tt, true)
local tttx_value = GetValue(tttx, 4)
local tt = {0x2646A0, 0x1C8, 0x198, 0xC0}
local ttty = S_Pointer(t, tt, true)
local ttty_value = GetValue(ttty, 4)
local tt = {0x2646A0, 0x1C8, 0x198, 0xC4}
local tttz = S_Pointer(t, tt, true)
local tttz_value = GetValue(tttz, 4)

-- 設定敵人和玩家的座標內存地址（這些值需要你自己搜索和確認）
local enemyXAddr = one_x
local enemyYAddr = one_y
local enemyZAddr = one_z

local playerXAddr = tttx
local playerYAddr = ttty
local playerZAddr = tttz
-- 主循環
local portal = gg.getRangesList(publicbase)[1].start
local oneself_addr = GotoPointer(portal + 0x7B50, {0x88, 0xC0})
local function setViewAngle(disx, disy, disz, distance)
  local angleX = math.deg(math.atan2(disx, disz)) 
  local angleY = math.deg(math.atan2(disy, distance))
gg.addListItems({{address = tttc ,flags = 16,value = angleX, freeze = true}})
gg.addListItems({{address = ttta,flags = 16,value = angleX, freeze = true},{address = tttaa,flags = 16,value = angleY, freeze = true}})
gg.addListItems({{address = tttb,flags = 16,value = angleX, freeze = true},{address = tttbb,flags = 16,value = angleY, freeze = true}})

end
while true do
local nearestDistance = theMaxdistance  -- 初始化最大距離
    local nearestEnemy = nil  -- 用來保存最近敵人的內存地址
    local nearestDisx, nearestDisy, nearestDisz = 0, 0, 0  -- 保存最近敵人的位置差異
  for lid = 0, 39 do
    local addr = oneself_addr + lid * 0x8
    local addrX = GotoPointer(addr + 0x0)
    local uidvalue = GetValue(addrX + 0x0, 4)
    local pdvalue = GetValue(addrX + 0xB0, 4)
    local pdteam = GetValue(addrX + 0x24, 4)
    local playerhp = GetValue(addrX + 0x10, 16)
    if ikummvstopaim == 1 then
    gg.addListItems({{address = tttc ,flags = 16,value = angleX, freeze = false}})
   gg.addListItems({{address = ttta,flags = 16,value = angleX, freeze = false},{address = tttaa,flags = 16,value = angleY, freeze = false}})
gg.addListItems({{address = tttb,flags = 16,value = angleX, freeze = false},{address = tttbb,flags = 16,value = angleY, freeze = false}})
    os.exit()
    return end 
    if uidvalue ~= 0 and uidvalue ~= nil then
    if pdvalue > 0 and pdvalue < 3 then
    if playerhp >0 then
       one_x = addrX + 0x14
       one_y = addrX + 0x18
       one_z = addrX + 0x1C

      -- 讀取敵人和玩家的位置
      local enemyX = getCoordinates(one_x)
      local enemyY = getCoordinates(one_y)
      local enemyZ = getCoordinates(one_z)
      
      local playerX = getCoordinates(playerXAddr)
      local playerY = getCoordinates(playerYAddr)
      local playerZ = getCoordinates(playerZAddr)
      
      local disx = playerX - enemyX
      local disy = playerY - enemyY
      local disz = playerZ - enemyZ

      -- 計算距離
      local distance = calculateDistance(enemyX, enemyY, enemyZ, playerX, playerY, playerZ)
      
      -- 如果距離小於某個閾值，則自動瞄準
      if distance < nearestDistance then
      if pdteam ~= noaimtean then
            nearestDistance = distance
            nearestEnemy = addrX
            nearestDisx, nearestDisy, nearestDisz = disx, disy, disz
       end
       end
      end
    end
  end
  end
  if nearestEnemy~=nil then
 setViewAngle(nearestDisx, nearestDisy, nearestDisz, nearestDistance)
 end
  -- 等待一段時間再進行下一次檢測
  gg.sleep(1)
end
end


