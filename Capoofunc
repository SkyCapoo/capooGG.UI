function handitemID()
	 gg.setRanges(4)
local dataType = 4
local tb1 = {{1117126656, 0},{256, -0x24},{65793, -0x6C},}
local tb2 = {{  -0x74 }, }
SearchWrite(tb1, tb2, dataType)
sln=gg.getResultCount()
if sln<1 then return end	
local nmb=gg.getResults(1)
	 sl=gg.getValues(nmb)
nnb=sl[1].value
gg.alert("手上物品的代碼為: "..nnb)
end

function MountInformation1()
