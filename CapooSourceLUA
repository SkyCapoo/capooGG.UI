local qnmboriginalValues = {}
function xeexg(szpy, qmxg) for x = 1, #(qmxg) do xgpy = szpy + qmxg[x]["offset"] xglx = qmxg[x]["type"] xgsz = qmxg[x]["value"] xgdj = qmxg[x]["freeze"] local currentValue = gg.getValues({{address = xgpy, flags = xglx}}) table.insert(qnmboriginalValues, {address = xgpy, flags = xglx, value = currentValue[1].value}) if xgdj == nil or xgdj == "" then gg.setValues({[1] = {address = xgpy, flags = xglx, value = xgsz}}) else gg.addListItems({[1] = {address = xgpy, flags = xglx, freeze = xgdj, value = xgsz}}) end xgsl = xgsl + 1 xgjg = true end end
function restoreqnmboriginalValues() if #qnmboriginalValues == 0 then gg.toast("æ²’æœ‰éœ€è¦é‚„åŸçš„å€¼") return end gg.setValues(qnmboriginalValues) qnmboriginalValues = {} end
function xcnmb(qmnb) gg.clearResults() gg.setRanges(qmnb[1]["memory"]) gg.searchNumber(qmnb[3]["value"], qmnb[3]["type"]) if gg.getResultCount() == 0 then gg.toast(qmnb[2]["name"] .. "å¼€å¯å¤±è´¥") else gg.refineNumber(qmnb[3]["value"], qmnb[3]["type"]) gg.refineNumber(qmnb[3]["value"], qmnb[3]["type"]) gg.refineNumber(qmnb[3]["value"], qmnb[3]["type"]) if gg.getResultCount() == 0 then gg.toast(qmnb[2]["name"] .. "å¼€å¯å¤±è´¥") else sl = gg.getResults(999999) sz = gg.getResultCount() xgsl = 0 if sz > 999999 then sz = 999999 end for i = 1, sz do pdsz = true for v = 4, #(qmnb) do if pdsz == true then pysz = {} pysz[1] = {} pysz[1].address = sl[i].address + qmnb[v]["offset"] pysz[1].flags = qmnb[v]["type"] szpy = gg.getValues(pysz) pdpd = qmnb[v]["lv"] .. ";" .. szpy[1].value szpd = split(pdpd, ";") tzszpd = szpd[1] pyszpd = szpd[2] if tzszpd == pyszpd then pdjg = true pdsz = true else pdjg = false pdsz = false end end end if pdjg == true then szpy = sl[i].address xeexg(szpy, qmxg) end end if xgjg == true then gg.toast(qmnb[2]["name"] .. "å¼€å¯æˆåŠŸ,æ”¹" .. xgsl .. "æ¡æ•°æ®") else gg.toast(qmnb[2]["name"] .. "å¼€å¯å¤±è´¥") end end end end



local saveFile = gg.EXT_STORAGE .. "/cheungKuitems_data.txt"

-- ä¿å­˜æ•¸æ“šåˆ°æ–‡ä»¶
function saveDataToFile(allitemsdata)
    local file = io.open(saveFile, "w")
    if file then
        file:write(table.concat(allitemsdata, ","))
        file:close()
        gg.toast("æ•¸æ“šå·²ä¿å­˜")
    else
        gg.toast("ä¿å­˜æ•¸æ“šå¤±æ•—")
    end
end
function loadDataFromFile()
    local file = io.open(saveFile, "r")
    if file then
        local content = file:read("*all")
        file:close()
        local allitemsdata = {}
        for value in string.gmatch(content, "([^,]+)") do
            table.insert(allitemsdata, tonumber(value))
        end
        if #allitemsdata == 3 then
            gg.toast("æ•¸æ“šåŠ è¼‰æˆåŠŸ")
            return allitemsdata
        else
            gg.toast("æ•¸æ“šæ–‡ä»¶æ ¼å¼éŒ¯èª¤ï¼Œä½¿ç”¨é»˜èªå€¼")
        end
    else
        gg.toast("æœªæ‰¾åˆ°ä¿å­˜çš„æ•¸æ“šï¼Œä½¿ç”¨é»˜èªå€¼")
    end
    return {12964, 1, 10500} -- é»˜èªæ•¸å€¼
end
function cloudcheangeitems()
    local cheungKuitems = loadDataFromFile()
result = gg.prompt(
        {"è¼¸å…¥å€‰åº«ç‰©å“ID", "è¼¸å…¥å€‰åº«ç‰©å“æ•¸é‡", "è¼¸å…¥ä¿®æ”¹ç‰©å“ID"},
        {[1] = cheungKuitems[1], [2] = cheungKuitems[2], [3] = cheungKuitems[3]},
        {[1] = "number", [2] = "number", [3] = "number"}
    )
    if not result then
        gg.toast("æ“ä½œå–æ¶ˆ")
        return
    end
    restoreqnmboriginalValues()
    -- ä¿å­˜ç”¨æˆ¶è¼¸å…¥çš„æ•¸æ“šåˆ°æ–‡ä»¶
    saveDataToFile(result)
 local CVitems = result[1]
 local CVnumber = result[2]
 local Changeitems = result[3]
 qmnb = {
{["memory"] = 4},
{["name"] = "ä¿®æ”¹å€‰åº«ç‰©å“"},
{["value"] =CVitems..".0", ["type"] = 64},
{["lv"] = 0, ["offset"] = 40, ["type"] = 4},
{["lv"] = CVnumber..".0", ["offset"] = 40, ["type"] = 64}
}
qmxg = {
{["value"] = Changeitems, ["offset"] = 0,["type"] = 64}
}
xcnmb(qmnb)
gg.sleep(10)
gg.toast(so({"è§£é–å–ç‰©1", Changeitems, 4, 4}, { {150323855360, 8, 32}, {"100~200", 208, 4}}, {{2, 8, 4}}))
gg.toast(so({"è§£é–å–ç‰©2", Changeitems, 4, 4}, { {"100~200", 208, 4}, {5, 220, 4}}, {{2, 8, 4}}))
end

function skinmedal()
local t = {publicbase, "Cb"}
local tt = {player_data_adress, 0x6C}
local ttt = S_Pointer(t, tt, true)
myskin = GetValue(ttt,4)
gg.toast(so({"åˆ·è¯éº—è®Šèº«",220,4,4},{{60,-4},{120,8},{180,-8},{120,4},{"1~200",-0x764},{"0~6",-0x438}},{{myskin,-0x764}}))
gg.toast("è¯éº—è®Šèº«åˆ·å–æˆåŠŸ")
end

function plantlvmedal()
qmnb = {
{["memory"] = 4},
{["name"] = "åˆ·èŒå£¯æˆé•·å‹³ç« "},
{["value"] =4634626229029306368, ["type"] = 32},
{["lv"] = 13963984896, ["offset"] = 4, ["type"] = 32},
{["lv"] = 117, ["offset"] = -4, ["type"] = 32},
{["lv"] = 4607182418800017408, ["offset"] = 40, ["type"] = 32},
{["lv"] = 13957595136, ["offset"] = 44, ["type"] = 32}}
qmxg = {
{["value"] = 25, ["offset"] = -40,["type"] = 64}}
xqmnb(qmnb)
gg.toast("èŒå£¯æˆé•·åˆ·å–æˆåŠŸ")
end


local hostitemsaveFilePath = gg.getFile() .. "_host_items_saved_data.txt"
-- å‡½æ•¸ï¼šä¿å­˜æ•¸æ“šåˆ°æ–‡ä»¶
local function hostitemssaveData(hostitemdata)
    local file = io.open(hostitemsaveFilePath, "w")
    if file then
        file:write(table.concat(hostitemdata, ","))
        file:close()
    end
end
-- å‡½æ•¸ï¼šè®€å–ä¿å­˜çš„æ•¸æ“š
function hostitemsloadData()
    local file = io.open(hostitemsaveFilePath, "r")
    if file then
        local content = file:read("*all")
        file:close()
        if content and #content > 0 then
            local hostitemdata = {}
            for value in string.gmatch(content, "([^,]+)") do
                table.insert(hostitemdata, tonumber(value))
            end
            return hostitemdata
        end
    end
    return nil
end
-- ä¸»å‡½æ•¸ï¼šè™•ç†ç‰©å“é‚è¼¯
function hostitemshack()
    -- è®€å–ä¿å­˜çš„æ•¸æ“š
    local defaultData = hostitemsloadData() or {15056, 200, 32000, 1}

    -- é¡¯ç¤ºæç¤ºçª—å£ï¼Œé è¨­ä½¿ç”¨ä¿å­˜çš„æ•¸æ“š
    local wp = gg.prompt(
        {"è¦å–çš„ç‰©å“ID", "ç‰©å“æ•¸é‡[1;200]", "ç‰©å“è€ä¹…[-1;32000]", "æ·»åŠ æ ¼æ•¸[1;8]"},
        {defaultData[1], defaultData[2], defaultData[3], defaultData[4]},
        {"number", "number", "number", "number"}
    )
    if not wp then
        gg.toast("æ“ä½œå–æ¶ˆ")
        return
    end

    -- ä¿å­˜ç”¨æˆ¶è¼¸å…¥çš„æ•¸æ“š
    hostitemssaveData(wp)

    -- è™•ç†ç‰©å“é‚è¼¯
    gg.toast(so({"ç²å–ç‰©å“ID", (wp[1]), 4, 4}, {{200, -432, 4}, {0, -0x4, 4}, {0, 0x8, 4}, {"100~200", 208, 4}}, {{nil, 0, 4, 2}}))
    for i, v in ipairs(gg.getListItems()) do
        if v.value == wp[1] then
            WPDM = {[1] = (v.address), [2] = (gg.getValues({{address = v.address + 0x5C, flags = 4}}))}
        end
    end
    if WPDM and WPDM[2] then
        gg.toast(so({"æ·»åŠ ç‰©å“", "(945804300~945804599)", 4, 4}, {{(wp[4] + 999), -4, 4}}, {{945804460~wp[2], 0, 4}, {wp[3], 4, 4}, {WPDM[1], 120, 4}, {(WPDM[2][1].value), 124, 4}}))
        gg.sleep(10)
        gg.toast("æ·»åŠ ç‰©å“å¼€å¯æˆåŠŸ")
    end
end



