function perspectivef1()
local t = {publicbase, "Cb"}
local tt = {0x1574E8,0x2E8,0x408}
local ttt = S_Pointer(t, tt, true)
gg.setValues({{address = ttt, flags = 16, value = 0}})
gg.toast("é€è¦–å·²é–‹å•Ÿ")
end


function perspectivef2()
local t = {publicbase, "Cb"}
local tt = {0x1574E8,0x2E8,0x408}
local ttt = S_Pointer(t, tt, true)
