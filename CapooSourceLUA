ikun=0
function autohead1()
ai = 0
repeat
ai = ai + AA3s
gg.sleep(10) 
local t = {publicbase, "Cb"}
local tt = {0x121B98,0x38,0x8}
local ttt = S_Pointer(t, tt, true)
gg.setValues({{address = ttt, flags = 16, value =ai}})
until (ai > 360)
if (ikun >0) then
return
else
return autohead1()
end end

function autohead2()
ikun=1
gg.toast("準心旋轉已關閉")
end

