-- 進程檢查
function checkProcess()
if gg.getTargetInfo()["label"] ~="Mini World" then
print("❗Error進程錯誤，注入失敗")
gg.toast("❗Error進程錯誤，注入失敗")
Lock.unUi()
gg.exit()
end
if gg.getTargetInfo()["versionName"] ~= targetVersion then
gg.alert("❗版本不是" .. targetVersion .."，注入失敗")
gg.toast("❗Error版本錯誤，注入失敗")
Lock.unUi()
gg.exit()
end
end
