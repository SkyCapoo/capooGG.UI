--写图片文件直链

_ENV["标题"] = 'SkyCapoo - UI'


_ENV["分页"] = {
	--导航栏分页名字和图标，添加表即可加页数，需要与功能配置页面数量对应
	'防閃/公告',
	'PvP功能',
	'聯機功能',
	'房主功能',
	'美化功能',
	'槍械功能',
	'輔助設置',
	--'设置',--这样添加新页面

}
init()    --传递变量勿动

uistart({ --每一个功能页面需要有上边分页名字对应，多出来的不显示，少了需要补充
	{     --1
		CAtext('公告', '#005BFF', '14sp'),



		CAtext(''..scriptAuthor..'_v'..scriptVersion..' 公益輔助', '#FFD306', '12sp', true),

