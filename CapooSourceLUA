function svip(af,ag) local gn,Dm={},{} for i,v in tostring(af):gmatch("%[(%s*%p*%d+)%] = '(.-)'") do gn[#gn+1]=v Dm[#Dm+1]=tonumber(i) end local GT=gg.choice(gn,2018,ag) local GY=Dm[GT] return GY end
