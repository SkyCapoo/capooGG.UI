local puttass = false
function player_TPCollision1()
stoptppd = 0
if puttass == false then
datauploadTime = gg.prompt({"è¾“å…¥å‚³é€é–“éš™æ™‚é–“"},{[1]="1"},{[1]="number"})[1]
gg.toast("å‚³é€å…‰ç’°å·²é–‹å•Ÿ")
end
    while true do
    puttass = true
    local gameportal = gg.getRangesList(publicbase)[1].start
    local playerself_addr = GotoPointer(gameportal + 0x7B50, {0x88, 0xC0})
    local Table = {}
    for lid = 0, 39 do
         addr = playerself_addr + lid * 0x8
        addrX = GotoPointer(addr + 0x0 )
       testAs = GetValue(addrX + 0xB0,4)
         if testAs ~= 2 then
            return player_TPCollision1() end
         if (stoptppd == "1") then
         os.exit()
          return end
        Table[#Table + 1] = addr
        one_x = GetValue(addrX + 0x14,4)
        one_y = GetValue(addrX + 0x18,4)
        one_z = GetValue(addrX + 0x1c,4)
        miniUID = GetValue(addrX + 0x0,4)
--è‡ªå·±çš„åº§æ¨™
gg.sleep(datauploadTime)
gg.toast("è¿·ä½ è™Ÿ:"..miniUID)
local t = {publicbase, "Cb"}
local tt = {0x2646A0, 0x1C8, 0x198, 0xBC}
local tttXX = S_Pointer(t, tt, true)

local tt = {0x2646A0, 0x1C8, 0x198, 0xC0}
local tttYY = S_Pointer(t, tt, true)

local tt = {0x2646A0, 0x1C8, 0x198, 0xC4}
local tttZZ = S_Pointer(t, tt, true)

local TPLocation = {tttXX,tttYY,tttZZ}
gg.setValues({{address = TPLocation[1],flags = 4,value = one_x},{address = TPLocation[2],flags = 4,value = one_y},{address = TPLocation[3],flags = 4,value = one_z}})
    end
    return Table, GotoPointer(playerself_addr)
end
end


