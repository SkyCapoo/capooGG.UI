function readmyname()
local t = {publicbase, "Cb"}
local tt = {player_data_adress, 0x49}
local ttt = S_Pointer(t, tt, true)

-- 定义内存地址和长度（这需要根据你的具体情况调整）
local address = ttt
local length = 64
-- 输出读取到的字符串
local result = readString(address, length)
gg.alert("遊戲名稱: " .. result)
end

function readplayer()
gg.toast("正在讀取玩家...")
-- 定义一些常量，比如玩家数量
local PLAYER_COUNT = 39
-- 定义一个函数，用于查找玩家位置
-- 读取内存中的字符串
function getPlayerPositions()
    local positions = {}
 local portal = gg.getRangesList(publicbase)[1].start
local oneself_addr = GotoPointer(portal + 0x7B50, {0x88, 0xC0})
    for i = 0, PLAYER_COUNT - 1 do
     local addr = oneself_addr + i * 0x8
    local baseAddress = GotoPointer(addr + 0x0)
    local uidvalue = GetValue(baseAddress + 0x0, 4)
    local pdvalue = GetValue(baseAddress + 0xB0, 4)
    local pdteam = GetValue(baseAddress + 0x24, 4)
    local playerHP = GetValue(baseAddress + 0x10, 16)
    if uidvalue ~= 0 and uidvalue ~= nil then
    if uidvalue > 100000000 and uidvalue < 3060036488 then
    if pdteam > -1 and pdteam < 7 then
    if pdvalue > 0 and pdvalue < 3 then
       one_gx = baseAddress + 0x14
       one_gy = baseAddress + 0x18
       one_gz = baseAddress + 0x1C
    -- 遍历所有玩家
        local x = gg.getValues({{address = one_gx, flags = gg.TYPE_DWORD}})[1].value
        local y = gg.getValues({{address = one_gy, flags = gg.TYPE_DWORD}})[1].value
        local z = gg.getValues({{address = one_gz, flags = gg.TYPE_DWORD}})[1].value
        tttname = baseAddress + 0x38
         address = tttname
         length = 32
 result = readString(address, length)
-- 输出读取到的字符串
table.insert(positions, {x = x/100, y = y/100, z = z/100,f= result,db= uidvalue,hp = playerHP})
    end
    end
    end
    end
    end
    return positions
end
capooikun = "--[玩家列表]--\n"
-- 定义一个函数，用于绘制玩家的框架
local function drawPlayerBoxes(positions)
    for i, pos in ipairs(positions) do
pname = string.format("\n[玩家%d]: ".. pos.f,i)
pminiuid = string.format(" \n[迷你號]: " .. pos.db)
php = string.format(" \n[血量]: " .. pos.hp)
pside = string.format(" \n[坐標]: (X%.2f, Y%.2f, Z%.2f)", i, pos.x, pos.y, pos.z)
capooikun = (capooikun .. pname .. pminiuid .. php .. pside)

end
gg.toast("请选择")
F = gg.alert(capooikun ,"取消","複製")
 if  F~=1 and F~=2 then
  gg.toast("你未选择开关默认取消")
    gg.toast("你未选择｛开 关｝")
  else 
  end
if  F== 1 then
os.exit()
 elseif  F== 2 then
 gg.copyText(capooikun)
        -- 这里可以添加实际绘制框架的代码
        -- 例如，可以调用绘图函数，绘制在游戏屏幕上
    end
end
    -- 获取玩家位置
     positions = getPlayerPositions()
    -- 绘制玩家框架
    drawPlayerBoxes(positions)
end



