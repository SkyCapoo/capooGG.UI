function playerhp()
gg.toast("请选择鎖定开关")
F = gg.alert("📌请选择是否鎖定 ","🔒鎖定","🔓普通")
 if  F~=1 and F~=2 then
  gg.toast("你未选择开关默认取消")
    gg.toast("你未选择｛开 关｝")
  else 
  end
if  F== 1 then
maxHP = gg.prompt({"输入目前血量:"}
,{[1]=100},{[1]="number"})[1]
gg.clearResults()
local t = {publicbase, "Cb"}
local tt = {0x2646A0, 0x1A0, 0x1E0, 0x78}
local ttt = S_Pointer(t, tt, true)
gg.setValues({{address = ttt, flags = 16, value = maxHP}})
gg.addListItems({{address = ttt, flags = 16, value = maxHP, freeze = true }})
gg.setValues({{address = ttt, flags = 16, value = maxHP}})
gg.toast("鎖定目前血量修改成功")

elseif  F== 2 then
maxHP = gg.prompt({"输入目前血量:"}
,{[1]=100},{[1]="number"})[1]
gg.clearResults()
local t = {publicbase, "Cb"}
local tt = {0x2646A0, 0x1A0, 0x1E0, 0x78}
local ttt = S_Pointer(t, tt, true)
gg.setValues({{address = ttt, flags = 16, value = maxHP}})
gg.addListItems({{address = ttt, flags = 16, value = maxHP, freeze = false }})
gg.setValues({{address = ttt, flags = 16, value = maxHP}})
gg.toast("目前血量修改成功")
end
end






