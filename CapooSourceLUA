function readmyname()
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x42BF8, 0x49}
local ttt = S_Pointer(t, tt, true)

-- 定义内存地址和长度（这需要根据你的具体情况调整）
local address = ttt
local length = 32

-- 读取内存中的字符串
-- 读取内存中的字符串
function readString(address, length)
    local char = {}
    for i = 0, length - 1 do
        local value = GetValue(address + i * 0x1, 1)
        if value ~= 0 then
            char[#char + 1] = value & 0xFF
        else break end
    end
    return string.char(table.unpack(char))
end

-- 输出读取到的字符串
local result = readString(address, length)
gg.alert("遊戲名稱: " .. result)
end

