local playerPos = {tttoox,tttooy,tttooz}
if (shutactionaimbot == "1") then
gg.addListItems({{address = angleAddra[1],flags = 16,value = setAngleX, freeze = false},{address = angleAddra[2],flags = 16,value = setAngleY, freeze = false}})
gg.addListItems({{address = angleAddrb[1],flags = 16,value = setAngleX, freeze = false},{address = angleAddrb[2],flags = 16,value = setAngleY, freeze = false}})
os.exit()
end
local AimFn = function(aimPos, playerPos, angleAddra)
local disx, disy, disz = aimPos[1] - playerPos[1], aimPos[2] - playerPos[2], aimPos[3] - playerPos[3]
local distance=math.sqrt(math.pow(disx,2)+math.pow(disz,2))
local setAngleX=math.deg(math.atan2(disx,disz))
local setAngleY=math.deg(math.atan2(disy,distance))

--gg.addListItems({{address = angleAddr[1],flags = 16,value = setAngleX, freeze = true},{address = angleAddr[2],flags = 16,value = setAngleY, freeze = true}})
gg.addListItems({{address = angleAddra[1],flags = 16,value = setAngleX, freeze = true},{address = angleAddra[2],flags = 16,value = setAngleY, freeze = true}})
gg.addListItems({{address = angleAddrb[1],flags = 16,value = setAngleX, freeze = true},{address = angleAddrb[2],flags = 16,value = setAngleY, freeze = true}})

end
AimFn(aimPos, playerPos, angleAddra)
end
end

