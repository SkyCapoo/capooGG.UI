function changename()
e = io.open("/sdcard/Capoo名字配置文件.txt", "rb") if e ==nil then mznb 
="" else
e = io.open("/sdcard/Capoo名字配置文件.txt"):read("*a") mznb = e:match("🍀(.-)🍀") if e == "" then mznb = "" end end

mnmc2 = ":w"
mnmcc = gg.prompt({"房主能彩名，修改後進入房間才有效果"}
,{[1]=mznb},{[1]="text"})[1]
--32
if not mnmcc then return end
mnmc = ":"..mnmcc
io.open("/sdcard/Capoo名字配置文件.txt", "w") io.open("/sdcard/Capoo名字配置文件.txt", "w"):write("🍀" .. mnmcc.. "🍀"):close()
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x42BF8, 0x49}
local ttt = S_Pointer(t, tt, true)

-- 定义目标字符串的内存地址
local address = ttt  -- 替换为实际的内存地址
local newString = mnmcc  -- 替换为你想要的文字

-- 将字符串转换为字节表
function stringToBytes(str)
    local bytes = {}
    for i = 1, #str do
        table.insert(bytes, string.byte(str, i))
    end
    table.insert(bytes, 0)  -- 添加字符串结束符
    return bytes
end

-- 修改内存中的字符串
function modifyString(address, newString)
    local bytes = stringToBytes(newString)
    local values = {}
    for i = 1, #bytes do
        table.insert(values, {address = address + i - 1, flags = gg.TYPE_BYTE, value = bytes[i]})
    end
    gg.setValues(values)
end
-- 调用函数修改字符串
modifyString(address, newString)
gg.toast("名字已修改为: " .. newString)
end

function fpsaimbot1()
    minijuid = gg.prompt({"输入自瞄目標迷你號,迷你號不需改成十位數"},{[1]="1000"},{[1]="number"})[1]
minijuid = minijuid + 1000000000
shutaim = 0
gg.setRanges(4)
local dataType = 4
local tb1 = {{minijuid, 0},{1120403456, 704},{1120403456,716},{1120403456,712},}
local tb2 = {{ 20 }, }
local tb3 = {{ 24 }, }
local tb4 = {{ 28 }, }
SearchWrite(tb1, tb2, dataType)
local nmbx=gg.getResults(1)
addrx = nmbx[1].address
	 sl=gg.getValues(nmbx)
nnbx=sl[1].value

SearchWrite(tb1, tb3, dataType)
local nmby=gg.getResults(1)
addry = nmby[1].address
	 sl=gg.getValues(nmby)
nnby=sl[1].value


SearchWrite(tb1, tb4, dataType)
local nmbz=gg.getResults(1)
addrz = nmbz[1].address 
	 sl=gg.getValues(nmbz)
nnbz=sl[1].value
while true do
if (shutaim == "1") then
os.exit()
end
--準心XY
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x1217D8,0x38,0x8}
local ttta = S_Pointer(t, tt, true)
local tt = {0x1217D8,0x38,0xc}
local tttb = S_Pointer(t, tt, true)
--自己的座標
local tt = {0x2642E0, 0x1C8, 0x198, 0xBC}
local ttt = S_Pointer(t, tt, true)
local tttx = GetValue(ttt, 4)
local tt = {0x2642E0, 0x1C8, 0x198, 0xC0}
local ttt = S_Pointer(t, tt, true)
local ttty = GetValue(ttt, 4)
local tt = {0x2642E0, 0x1C8, 0x198, 0xC4}
local ttt = S_Pointer(t, tt, true)
local tttz = GetValue(ttt, 4)
--自瞄目標的座標
local tttoox = GetValue(addrx, 4)
local tttooy = GetValue(addry, 4)
local tttooz = GetValue(addrz, 4)
local angleAddr = {ttta,tttb}
local aimPos = {tttx,ttty,tttz}
local playerPos = {tttoox,tttooy,tttooz}
local AimFn = function(aimPos, playerPos, angleAddr)
local disx, disy, disz = aimPos[1] - playerPos[1], aimPos[2] - playerPos[2], aimPos[3] - playerPos[3]
local distance=math.sqrt(math.pow(disx,2)+math.pow(disz,2))
local setAngleX=math.deg(math.atan2(disx,disz))-180
local setAngleY=math.deg(math.atan2(disy,distance))

--gg.addListItems({{address = angleAddr[1],flags = 16,value = setAngleX, freeze = true},{address = angleAddr[2],flags = 16,value = setAngleY, freeze = true}})
gg.setValues({{address = angleAddr[1],flags = 16,value = setAngleX},{address = angleAddr[2],flags = 16,value = setAngleY}})

end
AimFn(aimPos, playerPos, angleAddr)
end
end

function fpsaimbot2()
shutaim = 1
end

function Actionaimbot1()
    minijuid = gg.prompt({"输入自瞄目標迷你號,迷你號不需改成十位數"},{[1]="1000"},{[1]="number"})[1]
 minijuid = minijuid + 1000000000
shutactionaimbot = 0
gg.setRanges(4)
local dataType = 4
local tb1 = {{minijuid, 0},{1120403456, 704},{1120403456,716},{1120403456,712},}
local tb2 = {{ 20 }, }
local tb3 = {{ 24 }, }
local tb4 = {{ 28 }, }
SearchWrite(tb1, tb2, dataType)
local nmbx=gg.getResults(1)
addrx = nmbx[1].address
	 sl=gg.getValues(nmbx)
nnbx=sl[1].value
SearchWrite(tb1, tb3, dataType)
local nmby=gg.getResults(1)
addry = nmby[1].address
	 sl=gg.getValues(nmby)
nnby=sl[1].value
SearchWrite(tb1, tb4, dataType)
local nmbz=gg.getResults(1)
addrz = nmbz[1].address 
	 sl=gg.getValues(nmbz)
nnbz=sl[1].value
while true do
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x2642E0,0x368,0x88}
local tttxa = S_Pointer(t, tt, true)
local tt = {0x2642E0,0x250,0x50}
local tttxb = S_Pointer(t, tt, true)
--xpos
local tt = {0x2642E0,0x368,0x8c}
local tttya = S_Pointer(t, tt, true)
local tt = {0x2642E0,0x250,0x54}
local tttyb = S_Pointer(t, tt, true)
--ypos

local tt = {0x2642E0, 0x1C8, 0x198, 0xBC}
local ttt = S_Pointer(t, tt, true)
local tttx = GetValue(ttt, 4)
local tt = {0x2642E0, 0x1C8, 0x198, 0xC0}
local ttt = S_Pointer(t, tt, true)
local ttty = GetValue(ttt, 4)
local tt = {0x2642E0, 0x1C8, 0x198, 0xC4}
local ttt = S_Pointer(t, tt, true)
local tttz = GetValue(ttt, 4)
--自瞄目標的座標
local tttoox = GetValue(addrx, 4)
local tttooy = GetValue(addry, 4)
local tttooz = GetValue(addrz, 4)

local angleAddra = {tttxa,tttya}
local angleAddrb = {tttxb,tttyb}
local aimPos = {tttx,ttty,tttz}
local playerPos = {tttoox,tttooy,tttooz}
if (shutactionaimbot == "1") then
gg.addListItems({{address = angleAddra[1],flags = 16,value = setAngleX, freeze = false},{address = angleAddra[2],flags = 16,value = setAngleY, freeze = false}})
gg.addListItems({{address = angleAddrb[1],flags = 16,value = setAngleX, freeze = false},{address = angleAddrb[2],flags = 16,value = setAngleY, freeze = false}})
os.exit()
end
local AimFn = function(aimPos, playerPos, angleAddra)
local disx, disy, disz = aimPos[1] - playerPos[1], aimPos[2] - playerPos[2], aimPos[3] - playerPos[3]
local distance=math.sqrt(math.pow(disx,2)+math.pow(disz,2))
local setAngleX=math.deg(math.atan2(disx,disz))
local setAngleY=math.deg(math.atan2(disy,distance))

--gg.addListItems({{address = angleAddr[1],flags = 16,value = setAngleX, freeze = true},{address = angleAddr[2],flags = 16,value = setAngleY, freeze = true}})
gg.addListItems({{address = angleAddra[1],flags = 16,value = setAngleX, freeze = true},{address = angleAddra[2],flags = 16,value = setAngleY, freeze = true}})
gg.addListItems({{address = angleAddrb[1],flags = 16,value = setAngleX, freeze = true},{address = angleAddrb[2],flags = 16,value = setAngleY, freeze = true}})

end
AimFn(aimPos, playerPos, angleAddra)
end
end

function Actionaimbot2()
shutactionaimbot = 1

end

function changeteam()
teams = gg.prompt({"⬜0-白隊,🟥1-紅隊,🟦2-藍隊,🟩3綠隊,🟨4黃隊,🟧5橙隊,🟪6紫隊,👁️999旁觀者"}
,{[1]=1},{[1]="number"})[1]
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x10CCC8,0x48,0x4A8,0x70}
local ttt = S_Pointer(t, tt, true)
gg.setValues({{address = ttt, flags = 1, value = teams}})
end


function player_TPCollision1()
stoptppd = 0
if datauploadTime == nil then
datauploadTime = gg.prompt({"输入傳送間隙時間"},{[1]="1"},{[1]="number"})[1]
gg.toast("傳送光環開啟成功")
end
    local gameportal = gg.getRangesList("liblibGameApp.so:bss")[1].start
    local playerself_addr = GotoPointer(gameportal + 0x7B50, {0x88, 0xC0})
    while true do
    local Table = {}
    for lid = 0, 39 do
         addr = playerself_addr + lid * 0x8
        addrX = GotoPointer(addr + 0x0 )
       testAs = GetValue(addrX + 0xB0,4)
         if testAs ~= 2 then
            return player_TPCollision1() end
         if (stoptppd == "1") then
         os.exit()
          return end
        Table[#Table + 1] = addr
        one_x = GetValue(addrX + 0x14,4)
        one_y = GetValue(addrX + 0x18,4)
        one_z = GetValue(addrX + 0x1c,4)
        miniUID = GetValue(addrX + 0x0,4)
--自己的座標
gg.sleep(datauploadTime)
gg.toast("迷你號:"..miniUID)
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x2642E0, 0x1C8, 0x198, 0xBC}
local tttXX = S_Pointer(t, tt, true)

local tt = {0x2642E0, 0x1C8, 0x198, 0xC0}
local tttYY = S_Pointer(t, tt, true)

local tt = {0x2642E0, 0x1C8, 0x198, 0xC4}
local tttZZ = S_Pointer(t, tt, true)

local TPLocation = {tttXX,tttYY,tttZZ}
gg.setValues({{address = TPLocation[1],flags = 4,value = one_x},{address = TPLocation[2],flags = 4,value = one_y},{address = TPLocation[3],flags = 4,value = one_z}})
    end
    return Table, GotoPointer(playerself_addr)
end
end


function player_TPCollision2()
stoptppd = 1
datauploadTime = nil
gg.toast("傳送光環關閉成功")
end

function ca1() 
a9aa=o5v
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x434600,0x2B8,0x70,0x2D0,0x90}
local ttt = S_Pointer(t, tt, true)
gg.setValues({{address = ttt, flags = 4, value = a9a}})
gg.toast(a9aa.."人房間修改成功")
end

function changeroomplayerquantity()
SHff = gg.choice({
   "𒅒🔰開10人房間🔰𒅒",
   "𒅒🔰開20人房間🔰𒅒",
   "𒅒🔰開30人房間🔰𒅒",
   "𒅒🔰️開40人房間🔰𒅒",
},2018,os.date("𒅒開启成功后直接開房間就行了,建议每次開房間前都開一次这功能𒅒"))
if SHff==1 then a9a=7 o5v=10 ca1()end
if SHff==2 then a9a=8 o5b=20 ca1()end
if SHff==3 then a9a=9 o5v=30 ca1()end
if SHff==4 then a9a=10 o5v=40 ca1()end
XGCK=-1
end

local function getCoordinates(addr)
  local result = gg.getValues({{address = addr, flags = 4}})
  return result[1].value
end

-- 計算距離
local function calculateDistance(x1, y1, z1, x2, y2, z2)
  return math.sqrt((x2 - x1)^2 + (y2 - y1)^2 + (z2 - z1)^2)
end

-- 設定新的視角


function playerAimbot1()
readthevalueff = gg.prompt({"输入自瞄範圍,1=1格方塊","排除的隊伍(⬜0-白隊,🟥1-紅隊,🟦2-藍隊,🟩3綠隊,🟨4黃隊,🟧5橙隊,🟪6紫隊,👁️999旁觀者)"},{[1]="8",[2]="-1"},{[1]="number",[2]="number"})
theMaxdistance = readthevalueff[1] * 100
noaimtean = readthevalueff[2]
-- 準心XY
gg.toast("靠近玩家準心自瞄已成功")
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x1217D8, 0x38, 0x8}
local ttta = S_Pointer(t, tt, true)
local tt = {0x1217D8, 0x38, 0xc}
local tttb = S_Pointer(t, tt, true)

-- 自己的座標
local tt = {0x2642E0, 0x1C8, 0x198, 0xBC}
local tttx = S_Pointer(t, tt, true)
local tttx_value = GetValue(tttx, 4)
local tt = {0x2642E0, 0x1C8, 0x198, 0xC0}
local ttty = S_Pointer(t, tt, true)
local ttty_value = GetValue(ttty, 4)
local tt = {0x2642E0, 0x1C8, 0x198, 0xC4}
local tttz = S_Pointer(t, tt, true)
local tttz_value = GetValue(tttz, 4)

-- 設定敵人和玩家的座標內存地址（這些值需要你自己搜索和確認）
local enemyXAddr = one_x
local enemyYAddr = one_y
local enemyZAddr = one_z

local playerXAddr = tttx
local playerYAddr = ttty
local playerZAddr = tttz
-- 主循環
local portal = gg.getRangesList("liblibGameApp.so:bss")[1].start
local oneself_addr = GotoPointer(portal + 0x7B50, {0x88, 0xC0})
local function setViewAngle(disx, disy, disz, distance)
  local angleX = math.deg(math.atan2(disx, disz)) - 180
  local angleY = math.deg(math.atan2(disy, distance))
  gg.setValues({
    {address = ttta, flags = gg.TYPE_FLOAT, value = angleX},
    {address = tttb, flags = gg.TYPE_FLOAT, value = angleY}
  })
end
ikumstopaim = 0
while true do
  for lid = 0, 39 do
    local addr = oneself_addr + lid * 0x8
    local addrX = GotoPointer(addr + 0x0)
    local uidvalue = GetValue(addrX + 0x0, 4)
    local pdvalue = GetValue(addrX + 0xB0, 4)
    local pdteam = GetValue(addrX + 0x24, 4)
    if ikumstopaim == 1 then
    os.exit()
    return end 
    if uidvalue ~= 0 and uidvalue ~= nil then
    if pdvalue > 0 and pdvalue < 3 then
       one_x = addrX + 0x14
       one_y = addrX + 0x18
       one_z = addrX + 0x1C

      -- 讀取敵人和玩家的位置
      local enemyX = getCoordinates(one_x)
      local enemyY = getCoordinates(one_y)
      local enemyZ = getCoordinates(one_z)
      
      local playerX = getCoordinates(playerXAddr)
      local playerY = getCoordinates(playerYAddr)
      local playerZ = getCoordinates(playerZAddr)
      
      local disx = playerX - enemyX
      local disy = playerY - enemyY
      local disz = playerZ - enemyZ

      -- 計算距離
      local distance = calculateDistance(enemyX, enemyY, enemyZ, playerX, playerY, playerZ)
      
      -- 如果距離小於某個閾值，則自動瞄準
      if distance < theMaxdistance then
      if pdteam ~= noaimtean then
        setViewAngle(disx, disy, disz, distance)
       end
       end
      end
    end
  end
  -- 等待一段時間再進行下一次檢測
  gg.sleep(2)
end
end


function playermvAimbot1()
readthevalueff = gg.prompt({"输入自瞄範圍,1=1格方塊","排除的隊伍(⬜0-白隊,🟥1-紅隊,🟦2-藍隊,🟩3綠隊,🟨4黃隊,🟧5橙隊,🟪6紫隊,👁️999旁觀者)"},{[1]="8",[2]="-1"},{[1]="number",[2]="number"})
theMaxdistance = readthevalueff[1] * 100
noaimtean = readthevalueff[2]
ikummvstopaim = 0
-- 準心XY
gg.toast("靠近玩家動作自瞄已成功")
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x2642E0,0x368,0x88}
local ttta = S_Pointer(t, tt, true)
local tt = {0x2642E0,0x250,0x50}
local tttb = S_Pointer(t, tt, true)
--xpos
local tt = {0x2642E0,0x368,0x8c}
local tttaa = S_Pointer(t, tt, true)
local tt = {0x2642E0,0x250,0x54}
local tttbb = S_Pointer(t, tt, true)
-- 自己的座標
local tt = {0x2642E0, 0x1C8, 0x198, 0xBC}
local tttx = S_Pointer(t, tt, true)
local tttx_value = GetValue(tttx, 4)
local tt = {0x2642E0, 0x1C8, 0x198, 0xC0}
local ttty = S_Pointer(t, tt, true)
local ttty_value = GetValue(ttty, 4)
local tt = {0x2642E0, 0x1C8, 0x198, 0xC4}
local tttz = S_Pointer(t, tt, true)
local tttz_value = GetValue(tttz, 4)

-- 設定敵人和玩家的座標內存地址（這些值需要你自己搜索和確認）
local enemyXAddr = one_x
local enemyYAddr = one_y
local enemyZAddr = one_z

local playerXAddr = tttx
local playerYAddr = ttty
local playerZAddr = tttz
-- 主循環
local portal = gg.getRangesList("liblibGameApp.so:bss")[1].start
local oneself_addr = GotoPointer(portal + 0x7B50, {0x88, 0xC0})
local function setViewAngle(disx, disy, disz, distance)
  local angleX = math.deg(math.atan2(disx, disz)) 
  local angleY = math.deg(math.atan2(disy, distance))
  gg.setValues({
    {address = ttta, flags = gg.TYPE_FLOAT, value = angleX},
    {address = tttb, flags = gg.TYPE_FLOAT, value = angleY}
  })
gg.addListItems({{address = ttta,flags = 16,value = angleX, freeze = true},{address = tttb,flags = 16,value = angleY, freeze = true}})
gg.addListItems({{address = tttaa,flags = 16,value = angleX, freeze = true},{address = tttbb,flags = 16,value = angleY, freeze = true}})

end
while true do
  for lid = 0, 39 do
    local addr = oneself_addr + lid * 0x8
    local addrX = GotoPointer(addr + 0x0)
    local uidvalue = GetValue(addrX + 0x0, 4)
    local pdvalue = GetValue(addrX + 0xB0, 4)
    local pdteam = GetValue(addrX + 0x24, 4)
    if ikummvstopaim == 1 then
   gg.addListItems({{address = ttta,flags = 16,value = angleX, freeze = false},{address = tttb,flags = 16,value = angleY, freeze = false}})
gg.addListItems({{address = tttaa,flags = 16,value = angleX, freeze = false},{address = tttbb,flags = 16,value = angleY, freeze = false}})
    os.exit()
    return end 
    if uidvalue ~= 0 and uidvalue ~= nil then
    if pdvalue > 0 and pdvalue < 3 then
       one_x = addrX + 0x14
       one_y = addrX + 0x18
       one_z = addrX + 0x1C

      -- 讀取敵人和玩家的位置
      local enemyX = getCoordinates(one_x)
      local enemyY = getCoordinates(one_y)
      local enemyZ = getCoordinates(one_z)
      
      local playerX = getCoordinates(playerXAddr)
      local playerY = getCoordinates(playerYAddr)
      local playerZ = getCoordinates(playerZAddr)
      
      local disx = playerX - enemyX
      local disy = playerY - enemyY
      local disz = playerZ - enemyZ

      -- 計算距離
      local distance = calculateDistance(enemyX, enemyY, enemyZ, playerX, playerY, playerZ)
      
      -- 如果距離小於某個閾值，則自動瞄準
      if distance < theMaxdistance then
      if pdteam ~= noaimtean then
        setViewAngle(disx, disy, disz, distance)
       end
       end
      end
    end
  end
  -- 等待一段時間再進行下一次檢測
  gg.sleep(2)
   gg.addListItems({{address = ttta,flags = 16,value = angleX, freeze = false},{address = tttb,flags = 16,value = angleY, freeze = false}})
gg.addListItems({{address = tttaa,flags = 16,value = angleX, freeze = false},{address = tttbb,flags = 16,value = angleY, freeze = false}})
end
end


