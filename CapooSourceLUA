local hostitemsaveFilePath = gg.getFile() .. "_host_items_saved_data.txt"
-- 函數：保存數據到文件
local function hostitemssaveData(hostitemdata)
    local file = io.open(hostitemsaveFilePath, "w")
    if file then
        file:write(table.concat(hostitemdata, ","))
        file:close()
    end
end
-- 函數：讀取保存的數據
function hostitemsloadData()
    local file = io.open(hostitemsaveFilePath, "r")
    if file then
        local content = file:read("*all")
        file:close()
        if content and #content > 0 then
            local hostitemdata = {}
            for value in string.gmatch(content, "([^,]+)") do
                table.insert(hostitemdata, tonumber(value))
            end
            return hostitemdata
        end
    end
    return nil
end
-- 主函數：處理物品邏輯
function hostitemshack()
    -- 讀取保存的數據
    local defaultData = hostitemsloadData() or {15056, 200, 32000, 1}

    -- 顯示提示窗口，預設使用保存的數據
    local wp = gg.prompt(
        {"要取的物品ID", "物品數量[1;200]", "物品耐久[-1;32000]", "添加格數[1;8]"},
        {defaultData[1], defaultData[2], defaultData[3], defaultData[4]},
        {"number", "number", "number", "number"}
    )
    if not wp then
        gg.toast("操作取消")
        return
    end

    -- 保存用戶輸入的數據
    hostitemssaveData(wp)

    -- 處理物品邏輯
    gg.toast(so({"獲取物品ID", (wp[1]), 4, 4}, {{200, -432, 4}, {0, -0x4, 4}, {0, 0x8, 4}, {"100~200", 208, 4}}, {{nil, 0, 4, 2}}))
    for i, v in ipairs(gg.getListItems()) do
        if v.value == wp[1] then
            WPDM = {[1] = (v.address), [2] = (gg.getValues({{address = v.address + 0x5C, flags = 4}}))}
        end
    end
    if WPDM and WPDM[2] then
        gg.toast(so({"添加物品", "(945804300~945804599)", 4, 4}, {{(wp[4] + 999), -4, 4}}, {{945804460~wp[2], 0, 4}, {wp[3], 4, 4}, {WPDM[1], 120, 4}, {(WPDM[2][1].value), 124, 4}}))
        gg.sleep(10)
        gg.toast("添加物品开启成功")
    end
end



