gg.addListItems({{address = ttt, flags = 16, value = 8, freeze = true}})


gg.toast("隱身關閉成功")
end
function teleport()
mnsj={}
mnsj.qq=gg.prompt({"橫向座標","高度","竖向坐标"},mnsj.qqa)if mnsj.qq then 
mnsj.qqa=mnsj.qq
xc1=mnsj.qq[1].."00"
xc2=mnsj.qq[2].."00"
xc3=mnsj.qq[3].."00"
--x
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x2642E0, 0x1C8, 0x198, 0xBC}
local ttt = S_Pointer(t, tt, true)
gg.setValues({{address = ttt, flags = 4, value = xc1}})
--y
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x2642E0, 0x1C8, 0x198, 0xC0}
local ttt = S_Pointer(t, tt, true)
gg.setValues({{address = ttt, flags = 4, value = xc2}})
--z
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x2642E0, 0x1C8, 0x198, 0xC4}
local ttt = S_Pointer(t, tt, true)

gg.setValues({{address = ttt, flags = 4, value = xc3}})
gg.toast("定點傳送修改成功")
end
end





function playermaxhp()
HP = gg.prompt({"输入最大血量:"}
,{[1]=1000},{[1]="number"})[1]
gg.clearResults()
--最大生命值
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x2642E0, 0x1A0, 0x1E0, 0x7C}
local ttt = S_Pointer(t, tt, true)
gg.setValues({{address = ttt, flags = 16, value = HP}})
gg.toast("最大血量修改成功")
end









function playerhp()
gg.toast("请选择鎖定开关")
F = gg.alert("📌请选择是否鎖定 ","🔒鎖定","🔓普通")
 if  F~=1 and F~=2 then
  gg.toast("你未选择开关默认取消")
    gg.toast("你未选择｛开 关｝")
  else 
  end
if  F== 1 then
maxHP = gg.prompt({"输入目前血量:"}
,{[1]=100},{[1]="number"})[1]
gg.clearResults()
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x2642E0, 0x1A0, 0x1E0, 0x78}
local ttt = S_Pointer(t, tt, true)
gg.setValues({{address = ttt, flags = 16, value = maxHP}})
gg.addListItems({{address = ttt, flags = 16, value = maxHP, freeze = true }})
gg.setValues({{address = ttt, flags = 16, value = maxHP}})
gg.toast("鎖定目前血量修改成功")

elseif  F== 2 then
maxHP = gg.prompt({"输入目前血量:"}
,{[1]=100},{[1]="number"})[1]
gg.clearResults()
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x2642E0, 0x1A0, 0x1E0, 0x78}
local ttt = S_Pointer(t, tt, true)
gg.setValues({{address = ttt, flags = 16, value = maxHP}})
gg.addListItems({{address = ttt, flags = 16, value = maxHP, freeze = false }})
gg.setValues({{address = ttt, flags = 16, value = maxHP}})
gg.toast("目前血量修改成功")
end
end






