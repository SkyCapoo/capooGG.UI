			'日期時間',
			function()
				a() --调用了前置函数例子function a
			end),

		CAbutton(
			'輔助作者',
			function()
				gg.alert('https://youtube.com/@capoo_scriptmaker?si=E-4LwQYEMpwlTLfm')
			end,
			0xff0062ff), --第三参数颜色


		CAtext('内存分區'),

		CAswitch(
			"雙開防閃",
			function()
			 c()
            gg.alert('防閃已開啟')
			end,
			function()
            gg.alert('防閃已關閉')
			end,
			'進遊戲之前開'),

		CAswitch(
			"檢測進程",
			function()
               b()
			end,
			function()
            gg.alert('檢測關閉')
			end),





	}, { --2

		CAtext('PVP功能', '#005BFF', '14sp'),
