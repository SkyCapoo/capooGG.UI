local ttt = S_Pointer(t, tt, true)
gg.addListItems({{address = ttt, flags = 4, value = 257, freeze = true }})
gg.clearResults()
count=30.0+(154.0*1)-154.0
so({"坐骑",132.0,4,16},{{180.0,4,16},{count,36,16}},{{257,96,4,1},{MountID1,304,4,1}})
gg.toast("第一格坐騎添加/修改成功")
