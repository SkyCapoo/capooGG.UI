function godvoice()
AA3 = gg.prompt({"输入加数倍数：1=1倍/10=10倍"}
,{[1]=5},{[1]="number"})[1]
gg.clearResults()
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x1217D8,0x38,0xD0}
local ttt = S_Pointer(t, tt, true)
gg.setValues({{address = ttt, flags = 16, value = AA3}})
gg.clearResults()
gg.toast("背視角距離修改成功")
end
