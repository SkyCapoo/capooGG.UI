function starttheUI()
_ENV["悬浮窗图标"] = 'https://www.pnglog.com/7CSPjJ.png'
--写图片文件直链

_ENV["标题"] = 'SkyCapoo - UI'


_ENV["分页"] = {
	--导航栏分页名字和图标，添加表即可加页数，需要与功能配置页面数量对应
	'防閃/公告',
	'PvP功能',
	'聯機功能',
	'房主功能',
	'美化功能',
	'槍械功能',
	'輔助設置',
	--'设置',--这样添加新页面

}


