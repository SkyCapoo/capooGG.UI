function readmyname()
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x42FB8, 0x49}
local ttt = S_Pointer(t, tt, true)

-- 定义内存地址和长度（这需要根据你的具体情况调整）
local address = ttt
local length = 32
-- 输出读取到的字符串
local result = readString(address, length)
gg.alert("遊戲名稱: " .. result)
end

function readplayer()
gg.toast("正在讀取玩家...")
-- 定义一些常量，比如玩家数量
local PLAYER_COUNT = 39
-- 定义一个函数，用于查找玩家位置
-- 读取内存中的字符串
function getPlayerPositions()
    local positions = {}
 local portal = gg.getRangesList("liblibGameApp.so:bss")[1].start
local oneself_addr = GotoPointer(portal + 0x7B50, {0x88, 0xC0})
    for i = 0, PLAYER_COUNT - 1 do
     local addr = oneself_addr + i * 0x8
    local baseAddress = GotoPointer(addr + 0x0)
    local uidvalue = GetValue(baseAddress + 0x0, 4)
    local pdvalue = GetValue(baseAddress + 0xB0, 4)
    local pdteam = GetValue(baseAddress + 0x24, 4)
    local playerHP = GetValue(baseAddress + 0x10, 16)
    if uidvalue ~= 0 and uidvalue ~= nil then
    if uidvalue > 100000000 and uidvalue < 3060036488 then
    if pdteam > -1 and pdteam < 7 then
    if pdvalue > 0 and pdvalue < 3 then
       one_gx = baseAddress + 0x14
       one_gy = baseAddress + 0x18
       one_gz = baseAddress + 0x1C
    -- 遍历所有玩家
        local x = gg.getValues({{address = one_gx, flags = gg.TYPE_DWORD}})[1].value
        local y = gg.getValues({{address = one_gy, flags = gg.TYPE_DWORD}})[1].value
        local z = gg.getValues({{address = one_gz, flags = gg.TYPE_DWORD}})[1].value
        tttname = baseAddress + 0x38
         address = tttname
         length = 32
 result = readString(address, length)
-- 输出读取到的字符串
table.insert(positions, {x = x/100, y = y/100, z = z/100,f= result,db= uidvalue,hp = playerHP})
    end
    end
    end
    end
    end
    return positions
end
capooikun = "--[玩家列表]--\n"
-- 定义一个函数，用于绘制玩家的框架
local function drawPlayerBoxes(positions)
    for i, pos in ipairs(positions) do
pname = string.format("\n[玩家%d]: ".. pos.f,i)
pminiuid = string.format(" \n[迷你號]: " .. pos.db)
php = string.format(" \n[血量]: " .. pos.hp)
pside = string.format(" \n[坐標]: (X%.2f, Y%.2f, Z%.2f)", i, pos.x, pos.y, pos.z)
capooikun = (capooikun .. pname .. pminiuid .. php .. pside)

end
gg.toast("请选择")
F = gg.alert(capooikun ,"取消","複製")
 if  F~=1 and F~=2 then
  gg.toast("你未选择开关默认取消")
    gg.toast("你未选择｛开 关｝")
  else 
  end
if  F== 1 then
os.exit()
 elseif  F== 2 then
 gg.copyText(capooikun)
        -- 这里可以添加实际绘制框架的代码
        -- 例如，可以调用绘图函数，绘制在游戏屏幕上
    end
end
    -- 获取玩家位置
     positions = getPlayerPositions()
    -- 绘制玩家框架
    drawPlayerBoxes(positions)
end

function effectact1()
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x2646A0,0x250,0x558} 
local ttt = S_Pointer(t, tt,true)
gg.addListItems({{address = ttt, flags = 4, value = peffect1, freeze = true}}) 
gg.toast("人物特效修改成功")
end

function loopeffect1()  
menu1= gg.choice({
    "𒅒🔰紫色月亮🔰𒅒",
    "𒅒🔰碧琪特效🔰𒅒",
    "𒅒🔰绿色特效🔰𒅒",
    "𒅒🔰彩虹拖尾🔰𒅒",
    "𒅒🔰️蓝色特效🔰𒅒",
    "𒅒🔰️花小楼特效🔰𒅒",
    "𒅒🔰️冰公主特效🔰𒅒",
    "𒅒🔰️叶萝莉特效🔰𒅒",
   "𒅒🔰恢复动作️特效🔰𒅒", 
    "𒅒🎡返 回 主 页🎡𒅒",},2018,os.date("𒅒快来装逼啊！𒅒"))
if menu1==1 then peffect1=12 effectact1() end
if menu1==2 then peffect1=13 effectact1() end
if menu1==3 then peffect1=14 effectact1() end
if menu1==4 then peffect1=15 effectact1() end
if menu1==5 then peffect1=16 effectact1() end
if menu1==6 then peffect1=17 effectact1() end
if menu1==7 then peffect1=18 effectact1() end
if menu1==8 then peffect1=19 effectact1() end
if menu1==9 then peffect1=0 effectact1()  gg.clearList() end
if menu1==10 then Main1() end
XGCK=-1 end


function bullets1()
gg.toast("请选择鎖定开关")
F = gg.alert("📌请选择是否鎖定 ","🔒鎖定","🔓普通")
 if  F~=1 and F~=2 then
  gg.toast("你未选择开关默认取消")
    gg.toast("你未选择｛开 关｝")
  else 
  end
if  F== 1 then
changebullet = gg.prompt({"输入子彈數量:"}
,{[1]=114514},{[1]="number"})[1]
gg.clearResults()
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x4D00E0,0x188,0xA0,0xC0,0x160,0x10,0x14}
local ttt = S_Pointer(t, tt, true)
gg.setValues({{address = ttt, flags = 4, value = changebullet}})
gg.addListItems({{address = ttt, flags = 4, value = changebullet, freeze = true }})
gg.setValues({{address = ttt, flags = 4, value = changebullet}})
gg.toast("鎖定子彈數量修改成功")

elseif  F== 2 then
changebullet = gg.prompt({"输入子彈數量:"}
,{[1]=114514},{[1]="number"})[1]
gg.clearResults()
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x4D00E0,0x188,0xA0,0xC0,0x160,0x10,0x14}
local ttt = S_Pointer(t, tt, true)
gg.setValues({{address = ttt, flags = 4, value = changebullet}})
gg.addListItems({{address = ttt, flags = 4, value = changebullet, freeze = false }})
gg.setValues({{address = ttt, flags = 4, value = changebullet}})
gg.toast("子彈數量修改成功")
end
end

