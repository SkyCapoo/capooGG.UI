)[1] qmnb = {
{["memory"] = 1},
{["name"] = "宠物"},
{["value"] = 10001,["type"] = 4},
{["lv"] = 1,["offset"] = 4,["type"] = 4},
}
qmxg = {
{["value"] = gb,["offset"] = 8,["type"] = 4},
