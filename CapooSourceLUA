local running = false
function dologin()
gg.toast("ğŸ”°é»æ“Šä¿®æ”¹å™¨åœ–æ¨™ç™»é™¸è¼”åŠ©")
running = true
while running do
  if gg.isVisible(true) then
    gg.setVisible(false)
    checkplayerdata()
    choicelog = gg.choice({"ğŸ”°ç™»å…¥è¼”åŠ©ğŸ”°", "ğŸ”°éæª¢æ¸¬ğŸ”°", "âŒé€€å‡ºè¼”åŠ©âŒ"}, nil, "ğŸä½œè€…ï¼šSkyCapoo")
   if choicelog == 1 then
    if Myuid == 0 or Myuid == nil then
    gg.toast("è«‹å…ˆç™»å…¥éŠæˆ²")
    else
    running = false
    Sendtolog()
    starttheUI()
    end
   elseif choicelog == 2 then
      c()
   elseif choicelog == 3 then
      gg.toast("é€€å‡ºè¼”åŠ©")
      running = false
      gg.exit()
  end
  end
  gg.sleep(100)  -- æ¯0.1ç§’æª¢æŸ¥ä¸€æ¬¡æ‡¸æµ®çª—
end
end



function SkyCapoofileExists(name)
  local file = io.open(name, "r")
  if file then
    file:close()
    return true
  else
    return false
  end
end


function gameloging()
-- ç¬¬ä¸€æ¬¡é‹è¡Œæ™‚ï¼Œæç¤ºç”¨æˆ¶è¼¸å…¥åç¨±
if not SkyCapoofileExists(gg.getFile() .. "_Skyusername.txt") then
  loguser = gg.prompt({"è«‹è¨­å®šç”¨æˆ¶åç¨±"}, {[1] = "User"}, {[1] = "text"})[1]
  if not loguser then gg.alert("æœªè¨­å®šç”¨æˆ¶åç¨±") return end
  if loguser ~= "" then
    assfile = io.open(gg.getFile() .. "_Skyusername.txt", "w")
    assfile:write(loguser)
    assfile:close()
    gg.alert("ç”¨æˆ¶åç¨±å·²è¨­å®š: " .. loguser)
    username = loguser
    gg.alert("æ­¡è¿ä½¿ç”¨, ç”¨æˆ¶: " .. username)
    dologin()
  else
    gg.alert("æœªè¨­å®šç”¨æˆ¶åç¨±ï¼Œè¼”åŠ©é€€å‡º")
    gg.exit()
  end
else
  -- æ–‡ä»¶å­˜åœ¨æ™‚ï¼Œè‡ªå‹•è®€å–ç”¨æˆ¶åç¨±
  assfile = io.open(gg.getFile() .. "_Skyusername.txt", "r")
  username = assfile:read("*a")
  assfile:close()
  gg.alert("æ­¡è¿ä½¿ç”¨, ç”¨æˆ¶: " .. username)
  dologin()
end
end
gameloging()

Lock.Ui(invoke, nil, function(err) print(err) end)
