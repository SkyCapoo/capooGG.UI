function changeday() 
Day = gg.prompt({"输入天數:"}
,{[1]=100},{[1]="number"})[1]
gg.clearResults()
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x150B40,0x1F8}
local ttt = S_Pointer(t, tt, true)
gg.setValues({{address = ttt, flags = 4, value = Day*24000-23999}})
gg.clearResults()
gg.toast("天數修改成功")
end


