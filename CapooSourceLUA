-- 進程檢查
function checkProcess()
if gg.getTargetInfo()["label"] ~="Mini World" then
print("❗Error進程錯誤，注入失敗")
gg.toast("❗Error進程錯誤，注入失敗")
Lock.unUi()
gg.exit()
end
if gg.getTargetInfo()["versionName"] ~= targetVersion then
gg.alert("❗版本不是" .. targetVersion .."，注入失敗")
gg.toast("❗Error版本錯誤，注入失敗")
Lock.unUi()
gg.exit()
end
    if checkTarget ~= 0 then
        local info = gg.getTargetInfo()
        local check = false
        local current = false
        if checkTarget >= 1 then
            check = targetPkg
            current = info.packageName
        end
        if checkTarget >= 2 then
            check = check..' '..targetVersion..' ('..targetBuild..')'
            current = current..' '..info.versionName..' ('..info.versionCode..')'
        end
        if check ~= current then
            gg.alert('此腳本適用於 "'..targetName..'" ['..check..']。\n當前選擇的進程為 "'..info.label..'" ['..current..']。\n腳本將退出。')
            Lock.unUi()
            gg.exit()
        end
    end
end
checkProcess()  -- 檢查當前進程是否符合目標遊戲

function readString(address, length)
    local char = {}
    for i = 0, length - 1 do
        local value = GetValue(address + i * 0x1, 1)
        if value ~= 0 then
            char[#char + 1] = value & 0xFF
        else break end
    end
    return string.char(table.unpack(char))
end
