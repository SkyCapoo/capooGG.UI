local processes = gg.getListItems()
local gameProcess = nil
local gameProcessName = "mini world" -- 確保這是《迷你世界》的進程名稱

-- 使用 gg.getTargetInfo() 來獲取當前選中的進程信息
local targetInfo = gg.getTargetInfo()
local selectedProcess = targetInfo.label or ""
if string.find(selectedProcess:lower(), gameProcessName) then
else
    -- 遍歷進程列表，尋找包含 "miniworld" 的進程名稱
    for _, process in ipairs(processes) do
        if process.name ~= nil and string.find(process.name:lower(), gameProcessName) then
            gameProcess = process
            break
        end
    end

    -- 如果找到《迷你世界》進程，則選擇它
    if gameProcess then
        gg.setSelectedProcess(gameProcess.process)
    else
        gg.alert("未找到包含 '" .. gameProcessName .. "' 的進程")
        Lock.unUi()
        gg.exit()
    end
end


