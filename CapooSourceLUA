}),
}, { --7
	CAswitch(
		"é€€å‡ºè¼”åŠ©",
		function()
			Lock.unUi()
			gg.alert("ğŸ³ä½œè€…YT:SkyCapooğŸ³\nğŸ³ä½œè€…bilibili:SkyCapooğŸ³\n.....è¾…åŠ©å·²åœæ­¢è¿è¡Œ.....â¤ï¸ğŸ’›ğŸ’šğŸ’™ğŸ’œ") 
          print("ğŸ³ä½œè€…YT:SkyCapooğŸ³\nğŸ³ä½œè€…bilibili:SkyCapooğŸ³\nğŸ”†æˆåŠŸå…³é—­ï¼Œæ¬¢è¿ä¸‹æ¬¡ä½¿ç”¨")
          gg.playMusic("stop")
        	gg.exit()
		end,
		function()

		end),
}, { --8
},
})
end
running = false
function dologin()
running = true
while running do
  if gg.isVisible(true) then
    gg.setVisible(false)
    checkplayerdata()
    choicelog = gg.choice({"ğŸ”°ç™»å…¥è¼”åŠ©ğŸ”°", "ğŸ”°éæª¢æ¸¬ğŸ”°", "âŒé€€å‡ºè¼”åŠ©âŒ"}, nil, "ğŸä½œè€…ï¼šSkyCapoo")
   if choicelog == 1 then
    if Myuid == 0 or Myuid == nil then
    gg.toast("è«‹å…ˆç™»å…¥éŠæˆ²")
    else
    running = false
    Sendtolog()
    starttheUI()
    end
   elseif choicelog == 2 then
      c()
   elseif choicelog == 3 then
      gg.toast("é€€å‡ºè¼”åŠ©")
      running = false
      gg.exit()
  end
  end
  gg.sleep(100)  -- æ¯0.1ç§’æª¢æŸ¥ä¸€æ¬¡æ‡¸æµ®çª—
end
end


local SkyCapoofilename = gg.getFile():gsub("%.lua$", "_Skyusername.txt") -- ä¿å­˜ç”¨æˆ¶åçš„æ–‡ä»¶å
function SkyCapoofileExists(name)
  local file = io.open(name, "r")
  if file then
    file:close()
    return true
  else
    return false
  end
end


-- ç¬¬ä¸€æ¬¡é‹è¡Œæ™‚ï¼Œæç¤ºç”¨æˆ¶è¼¸å…¥åç¨±
if not SkyCapoofileExists(SkyCapoofilename) then
  loguser = gg.prompt({"è«‹è¨­å®šç”¨æˆ¶åç¨±"}, {[1] = "User"}, {[1] = "text"})
  if loguser and loguser[1] ~= "" then
    assfile = io.open(SkyCapoofilename, "w")
    assfile:write(loguser[1])
    assfile:close()
    gg.toast("ç”¨æˆ¶åç¨±å·²è¨­å®š: " .. loguser[1])
    gg.sleep(500)
    username = loguser[1]
    gg.toast("æ­¡è¿ä½¿ç”¨, ç”¨æˆ¶: " .. username)
    dologin()
  else
    gg.alert("æœªè¨­å®šç”¨æˆ¶åç¨±ï¼Œè¼”åŠ©é€€å‡º")
    gg.exit()
  end
else
  -- æ–‡ä»¶å­˜åœ¨æ™‚ï¼Œè‡ªå‹•è®€å–ç”¨æˆ¶åç¨±
  assfile = io.open(SkyCapoofilename, "r")
   username = assfile:read("*a")
  assfile:close()
  gg.toast("æ­¡è¿ä½¿ç”¨, ç”¨æˆ¶: " .. username)
  dologin()
end


Lock.Ui(invoke, nil, function(err) print(err) end)
